{
  "name": "@zhdgps/hi-components-web",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "src",
    "docs"
  ],
  "license": "ISC",
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:style": "stylelint \"./**/*.{css,scss,vue,html}\" --fix",
    "prepare": "husky install",
    "dev:docs": "pnpm run -C docs dev",
    "build:docs": "pnpm run -C docs build",
    "build:tools": "pnpm run -C packages/components build",
    "remove:modules": "rimraf node_modules && rimraf packages/*/node_modules"
  },
  "peerDependencies": {
  },
  "dependencies": {
    "@vue/shared": "^3.3.8",
    "@vueuse/core": "^10.6.1",
    "@zhdgps/assets": "workspace:^",
    "@zhdgps/constants": "workspace:^",
    "@zhdgps/vue3-tools": "workspace:^"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.1.1",
    "@iconify-json/carbon": "^1.1.24",
    "@iconify-json/ep": "^1.1.13",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/tsconfig": "^0.4.0",
    "@zhdgps/utils": "workspace:^",
    "eslint": "^8.54.0",
    "husky": "^8.0.3",
    "postcss": "^8.4.32",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.9",
    "rollup-plugin-visualizer": "^5.11.0",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-icons": "^0.18.1",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^4.4.5",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-inspect": "^0.8.1",
    "vue-tsc": "^1.8.5"
  }
}
